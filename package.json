{
	"name": "parse-tree",
	"displayName": "Parse tree",
	"description": "Access document syntax using tree-sitter",
	"version": "0.17.0",
	"publisher": "pokey",
	"repository": {
		"type": "git",
		"url": "https://github.com/pokey/vscode-parse-tree"
	},
	"license": "MIT",
	"extensionKind": [
		"workspace"
	],
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Programming Languages",
		"Themes",
		"Other"
	],
	"activationEvents": [
		"onLanguage:agda",
		"onLanguage:c",
		"onLanguage:clojure",
		"onLanguage:cpp",
		"onLanguage:css",
		"onLanguage:csharp",
		"onLanguage:go",
		"onLanguage:haskell",
		"onLanguage:html",
		"onLanguage:java",
		"onLanguage:javascript",
		"onLanguage:javascriptreact",
		"onLanguage:json",
		"onLanguage:jsonc",
		"onLanguage:latex",
		"onLanguage:markdown",
		"onLanguage:php",
		"onLanguage:python",
		"onLanguage:ruby",
		"onLanguage:rust",
		"onLanguage:scala",
		"onLanguage:scss",
		"onLanguage:shellscript",
		"onLanguage:typescript",
		"onLanguage:sparql",
		"onLanguage:typescriptreact",
		"onLanguage:xml",
		"onLanguage:yaml"
	],
	"main": "./out/extension.js",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		}
	},
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./ && make parsers",
		"watch": "tsc -watch -p ./",
		"preinstall": "make web-tree-sitter",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./out/test",
		"benchmark": "npm run compile && node ./out/benchmark",
		"debug": "npm run compile && node --nolazy --inspect-brk=9229 ./out/test",
		"build": "vsce package -o build.vsix",
		"publish": "vsce publish patch"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"electron-rebuild": "^2.3.5",
		"tree-sitter-agda": "github:pokey/tree-sitter-agda#e5fba6cabe8c7fc7993ced2b86704f3841215284",
		"tree-sitter-bash": "^0.19.0",
		"tree-sitter-c": "^0.19.0",
		"tree-sitter-css": "^0.19.0",
		"tree-sitter-c-sharp": "^0.19.0",
		"tree-sitter-cli": "^0.20.1",
		"tree-sitter-clojure": "github:sogaiu/tree-sitter-clojure#0.0.9",
		"tree-sitter-cpp": "^0.19.0",
		"tree-sitter-go": "^0.19.0",
		"tree-sitter-haskell": "github:tree-sitter/tree-sitter-haskell#1b54c3f39436bbded6593ac9e0103f9115bbbd2e",
		"tree-sitter-html": "^0.19.0",
		"tree-sitter-java": "^0.19.1",
		"tree-sitter-javascript": "^0.19.0",
		"tree-sitter-json": "^0.19.0",
		"tree-sitter-latex": "github:latex-lsp/tree-sitter-latex#104a5dea952d5f00150afd6a8436e6cad95ef718",
		"tree-sitter-markdown": "^0.7.1",
		"tree-sitter-php": "github:tree-sitter/tree-sitter-php#b9b719ded9a1c4f3fe29848306cfbd6ed4544282",
		"tree-sitter-python": "^0.19.0",
		"tree-sitter-ruby": "github:tree-sitter/tree-sitter-ruby#6334d6ab3d04a5672da695d3b155ca3301511f8d",
		"tree-sitter-rust": "^0.19.0",
		"tree-sitter-scala": "github:tree-sitter/tree-sitter-scala#8599058ef292e82203a1b23d10734dcbaf4d1b5c",
		"tree-sitter-scss": "github:serenadeai/tree-sitter-scss#c478c6868648eff49eb04a4df90d703dc45b312a",
		"tree-sitter-sparql": "^0.1.0",
		"tree-sitter-typescript": "^0.20.1",
		"tree-sitter-yaml": "^0.5.0",
		"tslint": "^6.0.0",
		"typescript": "^3.8.2",
		"vsce": "^1.73.0",
		"vscode": "^1.1.36"
	},
	"dependencies": {
		"jsonc-parser": "^2.1.0",
		"tar": ">=4.4.2",
		"web-tree-sitter": "file:vendor/web-tree-sitter/0.20.4"
	},
	"bundledDependencies": [
		"web-tree-sitter"
	]
}