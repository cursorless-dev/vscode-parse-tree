{
  "name": "parse-tree",
  "displayName": "Parse tree",
  "description": "Access document syntax using tree-sitter",
  "version": "0.47.0",
  "publisher": "pokey",
  "repository": {
    "type": "git",
    "url": "https://github.com/pokey/vscode-parse-tree"
  },
  "license": "MIT",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "engines": {
    "vscode": "^1.95.3"
  },
  "categories": [
    "Programming Languages",
    "Themes",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:agda",
    "onLanguage:c",
    "onLanguage:clojure",
    "onLanguage:cpp",
    "onLanguage:csharp",
    "onLanguage:css",
    "onLanguage:dart",
    "onLanguage:elixir",
    "onLanguage:elm",
    "onLanguage:gdscript",
    "onLanguage:gleam",
    "onLanguage:go",
    "onLanguage:haskell",
    "onLanguage:html",
    "onLanguage:java-properties",
    "onLanguage:java",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:json",
    "onLanguage:jsonc",
    "onLanguage:jsonl",
    "onLanguage:julia",
    "onLanguage:kotlin",
    "onLanguage:latex",
    "onLanguage:lua",
    "onLanguage:markdown",
    "onLanguage:nix",
    "onLanguage:perl",
    "onLanguage:php",
    "onLanguage:properties",
    "onLanguage:python",
    "onLanguage:r",
    "onLanguage:ruby",
    "onLanguage:rust",
    "onLanguage:scala",
    "onLanguage:scm",
    "onLanguage:scss",
    "onLanguage:shellscript",
    "onLanguage:sparql",
    "onLanguage:starlark",
    "onLanguage:swift",
    "onLanguage:talon-list",
    "onLanguage:talon",
    "onLanguage:terraform",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:xml",
    "onLanguage:yaml",
    "onLanguage:zig"
  ],
  "main": "./out/extension.js",
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "contributes": {},
  "scripts": {
    "clean": "rm -rf ./out && rm -rf parsers",
    "watch": "tsc -watch -p .",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run copy-assets",
    "copy-assets": "sh copy-assets.sh",
    "build": "vsce package -o build.vsix",
    "prettier": "prettier --write --list-different .",
    "lint": "eslint src && prettier --check ."
  },
  "devDependencies": {
    "@cursorless/tree-sitter-wasms": "0.7.0",
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.10.1",
    "@types/semver": "^7.3.12",
    "@types/vscode": "1.95.0",
    "@vscode/vsce": "^3.7.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint": "^9.34.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.41.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "semver": "^7.7.3",
    "web-tree-sitter": "^0.25.10"
  }
}
