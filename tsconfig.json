{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": ["es2020"],
    "outDir": "out",
    "rootDir": "src",
    "sourceMap": true,
    "strict": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true
  },
  "include": ["src"]
}
